<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Haxe">
  <template name="pv" value="public var $END$" description="public var" toReformat="false" toShortenFQNames="false">
    <context>
      <option name="HAXE" value="true"/>
    </context>
  </template>

  <template name="prv" value="private var $END$" description="private var" toReformat="false" toShortenFQNames="false">
    <context>
      <option name="HAXE" value="true"/>
    </context>
  </template>

  <template name="psv" value="public static var $END$" description="public static var" toReformat="false" toShortenFQNames="false">
    <context>
      <option name="HAXE" value="true"/>
    </context>
  </template>

  <template name="prsv" value="private static var $END$" description="private static var" toReformat="false" toShortenFQNames="false">
    <context>
      <option name="HAXE" value="true"/>
    </context>
  </template>

  <template name="pf" value="public function $NAME$($ARGS$):$RETURN_TYPE$ {&#10; $END$ &#10;}" description="public function"
            toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;name&quot;" alwaysStopAt="true"/>
    <variable name="ARGS" expression="" defaultValue="" alwaysStopAt="true"/>
    <variable name="RETURN_TYPE" expression="&quot;Void&quot;" defaultValue="&quot;Void&quot;" alwaysStopAt="true"/>
    <context>
      <option name="HAXE" value="true"/>
    </context>
  </template>

  <template name="prf" value="private function $NAME$($ARGS$):$RETURN_TYPE$ {&#10; $END$ &#10;}" description="private function"
            toReformat="true" toShortenFQNames="true">
